config system alias
edit "Delete Debug Alias -- Help"
set command "
config system alias
delete 'Debug HA -- Help'   
delete 'Debug Webfilter -- Help'   
delete 'Debug GUI -- Help'
delete 'Debug OSPF -- Help'
delete 'Debug FortiAnalyzer -- Help'
end
"
next
edit "Delete Debug Alias"
set command "
config system alias
delete 'Debug HA'
delete 'Debug Webfilter'
delete 'Debug GUI'
delete 'Debug OSPF'
delete 'Debug FortiAnalyzer' 
end
"
next
edit "Delete Info Alias -- Help"
set command "
config system alias
delete 'Info HA -- Help'   
delete 'Info Web Filter -- Help'
delete 'Info OSPF -- Help'
delete 'Info FortiAnalyzer -- Help'
end
"
next
edit "Delete Info Alias"
set command "
config system alias
delete 'Info HA' 
delete 'Info Web Filter'
delete 'Info OSPF'
delete 'Info FortiAnalyzer'
end
"
next
edit "Delete TAC Alias -- Help"
set command "
config system alias
delete 'TAC FortiAnalyzer -- Help'   
end
"
next
edit "Delete TAC Alias"
set command "
config system alias
delete 'TAC FortiAnalyzer' 
end
"
next
edit "DELETE ALL"
set command "
alias 'Delete Debug Alias -- Help'
alias 'Delete Debug Alias'
alias 'Delete Info Alias -- Help'
alias 'Delete Info Alias'
alias 'Delete TAC Alias -- Help'
alias 'Delete TAC Alias'
"
end
